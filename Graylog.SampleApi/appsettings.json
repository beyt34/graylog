{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "GELF": {
      "Host": "127.0.0.1",
      "Port": 12201,                 // Not required if using default 12201. 
      "Protocol": "HTTP",             // Not required if using default UDP. 
      "LogLevel": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    }
  },
  "AllowedHosts": "*"
}
